{
    "name": "involta/intimstaff",
    "description": "intimstaff",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "involta-design/yii2-psr": "dev-master",
        "laminas/laminas-diactoros": "^2.4",
        "laminas/laminas-httphandlerrunner": "^1.2",
        "monolog/monolog": "^2.1",
        "yiisoft/http": "^1.0",
        "yiisoft/yii2": "^2.0.38"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
